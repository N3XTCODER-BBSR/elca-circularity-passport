generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Passport {
  id                         String   @id @default(cuid())
  name                       String
  buildingStructureId        String
  address                    String
  buildingYear               Int
  buildingType               String
  numberOfFloors             Int
  nrf                        String
  bgf                        String
  bri                        String
  plotArea                   String
  percentageOfSealedLandArea Int
  totalMassOfBuilding        Int
  dataQuality                String
  authorName                 String
  issueDate                  DateTime
  expiryDate                 DateTime
}
